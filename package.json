{
  "name": "item-template",
  "version": "0.0.0",
  "packageManager": "pnpm@7.21.0",
  "license": "MIT",
  "homepage": "https://github.com/hengshanMWC/item-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hengshanMWC/item-template.git"
  },
  "bugs": {
    "url": "https://github.com/hengshanMWC/item-template/issues"
  },
  "main": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "init": "pnpm i && npm run prepare",
    "release": "esno ./scripts/release",
    "build": "tsup src/index.ts --format esm,cjs --dts-resolve --target node14 --clean --splitting",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "lint": "eslint --ext=js,ts,json,md,tsx .",
    "lint:fix": "eslint --fix --ext=js,ts,json,md,tsx .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.38.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitest/coverage-c8": "^0.31.0",
    "bumpp": "^9.1.0",
    "colors": "^1.4.0",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "tsup": "^7.1.0",
    "typescript": "^5.0.4",
    "vitepress": "1.0.0-alpha.29",
    "vitest": "^0.31.1"
  },
  "lint-staged": {
    "{server,client}/*.{js,ts,json,md,tsx}": "pnpm lint"
  }
}
